at_fn_check_prepare_dynamic "$COBCRUN_DIRECT ./prog" "run_file.at:4128"
( $at_check_trace; $COBCRUN_DIRECT ./prog
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
