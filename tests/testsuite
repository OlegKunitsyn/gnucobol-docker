at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/listings.at:1885"
$at_failed && at_fn_log_failure  \
"prog.lst"
$at_traceon; }


{ set +x
