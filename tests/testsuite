at_fn_check_prepare_dynamic "$COMPILE -fdump=FD,LS prog.cob -o prog_fdls" "run_misc.at:9370"
( $at_check_trace; $COMPILE -fdump=FD,LS prog.cob -o prog_fdls
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/run_misc.at:9370"
$at_failed && at_fn_log_failure  \
"./tstdump.txt"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/run_misc.at:9372: COB_DUMP_FILE=tstdump_fdls.txt \\
\$COBCRUN_DIRECT ./prog_fdls"
at_fn_check_prepare_notrace 'an embedded newline' "run_misc.at:9372"
( $at_check_trace; COB_DUMP_FILE=tstdump_fdls.txt \
$COBCRUN_DIRECT ./prog_fdls
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
