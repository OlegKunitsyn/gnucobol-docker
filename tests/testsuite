( $at_check_trace; $COBC -fsyntax-only -Wadditional prog.cob
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
echo >>"$at_stderr"; $as_echo "prog.cob:5: warning: PERFORM FOREVER since UNTIL is always FALSE
" | \
  $at_diff - "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/used_binaries.at:59"
$at_failed && at_fn_log_failure
$at_traceon; }

# which in this case is also included in -Wall
{ set +x
