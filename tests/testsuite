$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/used_binaries.at:57"
$at_failed && at_fn_log_failure
$at_traceon; }

# increasing to more warnings
{ set +x
$as_echo "$at_srcdir/used_binaries.at:59: \$COBC -fsyntax-only -Wadditional prog.cob"
