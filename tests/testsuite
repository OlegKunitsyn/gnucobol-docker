{ set +x
$as_echo "$at_srcdir/run_misc.at:8804: diff reference trace.txt"
at_fn_check_prepare_trace "run_misc.at:8804"
( $at_check_trace; diff reference trace.txt
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/run_misc.at:8804"
$at_failed && at_fn_log_failure  \
"./trace.txt"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/run_misc.at:8806: \$COMPILE -ftrace prog.cob -o prog_s"
at_fn_check_prepare_dynamic "$COMPILE -ftrace prog.cob -o prog_s" "run_misc.at:8806"
( $at_check_trace; $COMPILE -ftrace prog.cob -o prog_s
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/run_misc.at:8806"
$at_failed && at_fn_log_failure  \
"./trace.txt"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/run_misc.at:8808: COB_TRACE_FILE=+trace_append.txt \\
COB_SET_TRACE=Y \\
COB_TRACE_FORMAT=\"%S and now ... %L\" \\
\$COBCRUN_DIRECT ./prog_s"
at_fn_check_prepare_notrace 'an embedded newline' "run_misc.at:8808"
( $at_check_trace; COB_TRACE_FILE=+trace_append.txt \
COB_SET_TRACE=Y \
COB_TRACE_FORMAT="%S and now ... %L" \
$COBCRUN_DIRECT ./prog_s
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
