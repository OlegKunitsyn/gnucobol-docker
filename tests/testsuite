cat >prog.cob <<'_ATEOF'

       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       DATA             DIVISION.
       WORKING-STORAGE  SECTION.
       COPY copy1.
       PROCEDURE        DIVISION.
           DISPLAY TEST-VAR.
           STOP RUN.
_ATEOF


cat >copy1.CPY <<'_ATEOF'

       COPY copy2.
       01 TEST-VAR  PIC X(2) VALUE "V1".
_ATEOF


cat >copy2.CPY <<'_ATEOF'

       01 TEST-VAR2 PIC X(2) VALUE "V2".
       COPY copy3.
_ATEOF


cat >copy3.CPY <<'_ATEOF'
       COPY "copy1.CPY".
       01 TEST-VAR3 PIC X(2) VALUE "V3".
_ATEOF


{ set +x
