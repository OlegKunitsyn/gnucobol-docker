cat >prog.cob <<'_ATEOF'

        IDENTIFICATION DIVISION.
        PROGRAM-ID. prog.
        PROCEDURE DIVISION.
        PERFORM UNTIL 0 = 1
           DISPLAY 'BLA'
        END-PERFORM.
_ATEOF

{ set +x
$as_echo "$at_srcdir/used_binaries.at:57: \$COBC -fsyntax-only prog.cob"
